<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%    // DON'T FREAK OUT!!! This scriptlet code will go away once    // we have a model and controller in place...        String[][] carList = {            {"Toyota", "Camry", "2005"},            {"Toyota", "Corolla", "1999"},            {"Ford", "Explorer", "2005"}    };        pageContext.setAttribute("carList", carList);%><html><body>
  <table border="1">    <tr>      <th bgcolor="cccccc" align="left">Make</th>      <th bgcolor="cccccc" align="left">Model</th>      <th bgcolor="cccccc" align="right">Model Year</th>    </tr>        <c:forEach items='${carList}' var='car'>      <tr>      <td align="left">${car[0]}</td>      <td align="left">${car[1]}</td>      <td align="right">${car[2]}</td>      </tr>    </c:forEach>  </table></body></html>